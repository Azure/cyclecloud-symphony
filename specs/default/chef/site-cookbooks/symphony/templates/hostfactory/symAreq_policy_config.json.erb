{
    "scaling_policy":[
        {
            "name": "throughput",
            "description": "Throughput is the number of tasks executed per minutes per slot",
            "warmup_time": <%= node['symphony']['hostfactory']['requestors']['symA']['scaling_policy']['warmup_time'] %>,
            "history_expiry_time": 60,
            "active_task_moving_avg": 5,
            "desired_task_complete_duration": <%= node['symphony']['hostfactory']['requestors']['symA']['scaling_policy']['desired_task_complete_duration'] %>,
            "max_cores_per_hour": 0,
            "ego_host_startup_time": 5,
            "ego_failover_timeout": 10
        }
    ],
    "host_return_policy":[
        {
            "name": "<%= node['symphony']['hostfactory']['requestors']['symA']['host_return_policy']['name'] %>",
            "description": "Return cloud hosts closer to the end of billing interval",
            "billing_interval": <%= node['symphony']['hostfactory']['requestors']['symA']['host_return_policy']['billing_interval'] %>,
            "return_interval": <%= node['symphony']['hostfactory']['requestors']['symA']['host_return_policy']['return_interval'] %>,
            "force_return_interval": <%= node['symphony']['hostfactory']['requestors']['symA']['host_return_policy']['force_return_interval'] %>,
            "return_idle_only": <%= node['symphony']['hostfactory']['requestors']['symA']['host_return_policy']['return_idle_only'] %>
        }
    ],
    "azurecc": true
}
